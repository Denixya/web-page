{
  "version": 2,
  "builds": [
    {
      "src": "api/wow.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/WEB-PAGE/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/wow.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/WEB-PAGE/$1"
    }
  ]
}
